    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjsplus/redirect": "^1.0.0",
    "@prisma/client": "^2.25.0",
    "@types/nodemailer": "^6.4.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.5",
    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "nodemailer": "^6.6.2",
    "nodemon": "^2.0.7",
    "object-hash": "^2.2.0",
    "passport": "^0.4.1",
