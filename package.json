    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.18",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/object-hash": "^2.1.0",
    "@types/passport-google-oauth20": "^2.0.8",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2": "^1.4.10",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.5",
    "prettier": "^2.3.1",
    "prisma": "^2.25.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^4.3.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
